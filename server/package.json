{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.ts",
    "test:watch": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.ts --watch",
    "test:clear": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.ts --clearCache"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "5.0.0-beta.1",
    "path-to-regexp": "6.2.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "jest-express": "^1.12.0",
    "jest-ts-webcompat-resolver": "^1.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.8.1",
    "typescript": "^5.8.3"
  },
  "jest":{
    "extensionsToTreatAsEsm":[".ts"],
    "globals":{
      "ts-jest":{
        "useESM":true
      }
    }
  }
}
